{
  "name": "tuneOut",
  "description": "Audio stream music identification extension",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./background.bundle.js"
    ]
  },
  "options_page": "./idx-options.html",
  "browser_action": {
    "default_popup": "./idx-popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab",
    "tabCapture"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}